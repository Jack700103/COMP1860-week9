CHIP TrafficLight2 {
    IN PowerOn;
    OUT S[2];
    PARTS:
    DFF(in=nextS0, out=S0);
    DFF(in=nextS1, out=S1);
    
    Or(a=S0, b=false, out=S[0]);
    Or(a=S1, b=false, out=S[1]);
    
    Not(in=S0, out=notS0);
    Xor(a=S1, b=S0, out=xorOut);
    
    Mux(a=notS0, b=false, sel=PowerOn, out=nextS0);
    Mux(a=xorOut, b=false, sel=PowerOn, out=nextS1);
}